<h1>Listing espacios</h1>

<table>
  <tr>
    <th>Login</th>
    <th>Mail</th>
    <th>Created at</th>
  </tr>

<% User.all.each do |user| %>
  <tr>
    <td><%=h user.login %></td>
    <td><%=h user.email %></td>
    <td><%=h user.created_at %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo usuario', :controller => 'users', :action => 'new' %>
