<h1>Listing plans</h1>

<table>
  <tr>
    <th>Codigo</th>
    <th>Codigo de Carrera</th>
    <th>Nombre</th>
  </tr>

<% @plans.each do |plan| %>
  <tr>
    <td><%=h plan.codigo %></td>
    <td><%=h plan.codigo_carrera %></td>
    <td><%=h plan.nombre %></td>
    <td><%= link_to 'Show', plan %></td>
    <td><%= link_to 'Edit', edit_plan_path(plan) %></td>
    <td><%= link_to 'Destroy', plan, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo plan', new_plan_path %><br>
<%= link_to 'Volver al indice', root_path %>
