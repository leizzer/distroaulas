<h1>New materia</h1>

<% form_for(@materia) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :codigo %><br />
    <%= f.text_field :codigo %>
  </p>
  <p>
    <%= f.label :codigo_carrera %><br />
    <%= f.text_field :codigo_carrera %>
  </p>
  <p>
    <%= f.label :codigo_plan %><br />
    <%= f.text_field :codigo_plan %>
  </p>
  <p>
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', materias_path %>
