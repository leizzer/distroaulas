class Evento < ActiveRecord::Base
  validates_numericality_of :interval
#  before_save :end_date_set

  belongs_to :espacio

 def end_date_set

   Evento.dtend = DateTime.parse(Evento.dtstart.year.to_s + Evento.dtstart.month.to_s + Evento.dtstart.day.to_s + "T" + Evento.dtend.hour.to_s + self.dtend.min.to_s)
 end
end
